{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "loose": true
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "corejs": 3 }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-proposal-export-default-from"
  ]
}
